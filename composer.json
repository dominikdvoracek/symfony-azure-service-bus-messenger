{
    "name": "santn/azure-service-bus-messenger",
    "description": "Symfony Azure Service Bus extension Messenger Bridge",
    "type": "symfony-messenger-bridge",
    "require": {
        "php": ">=7.2.5",
        "symfony/messenger": "^4.3|^5.0|^6.0",
        "psr/log": "^1|^2|^3",
        "symfony/service-contracts": "2.5.x-dev",
        "microsoft/windowsazure": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "symfony/serializer": "5.4.x-dev",
        "symfony/property-access": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "San Tran",
            "email": "solesantn@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Symfony\\Component\\Messenger\\Bridge\\AzureServiceBus\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
